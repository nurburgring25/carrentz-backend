<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console target="SYSTEM_OUT" name="CONSOLE">
            <PatternLayout pattern="%-5level %logger{36} - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="CONSOLE" />
        </Root>
    </Loggers>
</Configuration>